<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#000000">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        app:srcCompat="@drawable/levelback" />

    <RelativeLayout
        android:id="@+id/layoutOndsae2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignBottom="@+id/topLeftCenter2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/topLeftCenter2"
        android:layout_toLeftOf="@+id/topLeftCenter2">

        <TextView
            android:id="@+id/dddddddd"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_centerInParent="true"
            android:text="TextView" />

        <RelativeLayout
            android:id="@+id/layoutOneOwwwne2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignBottom="@+id/dddddddd"
            android:layout_toStartOf="@+id/dddddddd"
            android:layout_toLeftOf="@+id/dddddddd">

            <TextView
                android:id="@+id/topLeftCenter3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_centerInParent="true"
                android:text="TextView" />

            <RelativeLayout
                android:id="@+id/layoutOneOdsadne2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignBottom="@+id/topLeftCenter3"
                android:layout_toStartOf="@+id/topLeftCenter3"
                android:layout_toLeftOf="@+id/topLeftCenter3">

                <TextView
                    android:id="@+id/topLeftCenter4"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_centerInParent="true"
                    android:text="TextView" />

                <RelativeLayout
                    android:id="@+id/layoutOneOdsane2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignTop="@+id/topLeftCenter4"
                    android:layout_toStartOf="@+id/topLeftCenter4"
                    android:layout_toLeftOf="@+id/topLeftCenter4">

                    <Button
                        android:id="@+id/backbtn"
                        android:layout_width="45dp"
                        android:layout_height="30dp"
                        android:layout_marginBottom="-1dp"
                        android:background="@drawable/backbutton" />
                </RelativeLayout>
            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="435px"
            android:layout_height="588px"
            android:layout_marginRight="25dp"
            app:srcCompat="@drawable/levelone" />


        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="433px"
            android:layout_height="588px"
            android:layout_gravity="center_vertical"
            app:srcCompat="@drawable/leveltwo" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="433px"
            android:layout_height="588px"
            android:layout_marginLeft="25dp"
            app:srcCompat="@drawable/levelthree" />
    </LinearLayout>
</FrameLayout>

